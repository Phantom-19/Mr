import time,os,sys
 
def auto(s):
    for c in s + '\n':
             sys.stdout.write(c)
             sys.stdout.flush()
             time.sleep(0.007)
   
def Street(z):
    for e in z + '\n':
             sys.stdout.write(e)
             sys.stdout.flush()
             time.sleep(0.009)

    
